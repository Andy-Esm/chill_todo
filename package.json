    "lint:code:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:style": "stylelint \"src/{**/*,*}.scss\"",
    "lint:style:fix": "stylelint \"src/{**/*,*}.scss\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "server": "json-server-auth -w server/db.json server/routes.json -p 5001 --middlewares server/middlewares.js",
    "dev:server": "concurrently \"npm start\" \"npm run server\""
